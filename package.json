{
  "name": "www",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "npm run build && npm run serve",
    "start:dist": "npm run build && npm run serve:dist",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:server": "tsc --project webserver/tsconfig.json",
    "preview": "vite preview",
    "lint": "eslint --max-warnings=0 src",
    "serve:cdn": "ts-node --esm ./webserver/cdn/http.ts",
    "serve:ssr": "ts-node --esm ./webserver/ssr/http.ts",
    "serve:dist:cdn": "node dist-webserver/cdn/http.js",
    "serve:dist:ssr": "node dist-webserver/ssr/http.js",
    "serve": "concurrently \"npm run serve:cdn\" \"npm run serve:ssr\"",
    "serve:dist": "concurrently \"npm run serve:dist:cdn\" \"npm run serve:dist:ssr\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@loadable/component": "^5.15.2",
    "@reduxjs/toolkit": "^1.9.1",
    "@vendia/serverless-express": "^4.10.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql-request": "^5.1.0",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.42.1",
    "react-imported-component": "^6.5.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.7.0",
    "tailwind-merge": "^1.8.1",
    "vite-plugin-ssr": "^0.4.71",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/loadable__component": "^5.13.4",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-offline": "^12.0.4",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "updates": "^13.2.7",
    "vite": "^4.0.0",
    "vite-plugin-eslint": "^1.8.1"
  }
}
